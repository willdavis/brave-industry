<div class="markets-show" id="<%= @market.region_id %>">
  <div class="row">
		<div class="col-xs-12">
			<table class="market-header">
				<tr>
					<td class="item-image"><img src="<%= @market.item.image['thumb'] %>" width='64' height='64' /></td>
					<td class="item-name"><h3><%= @market.item.name %></h3></td>
				</tr>
				<tr>
				  <td class="region-name"><%= @market.region.name %></td>
				</tr>
			</table>
		</div>
	</div>
</div>
