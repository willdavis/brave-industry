<%= link_to "Brave Industry", root_path, :class => 'brand' %>
<ul class="nav">
  <li><%= link_to 'Blueprints', blueprints_path %></li>
</ul>
<form class="navbar-form pull-right" role="search">
  <div class="form-group">
  	<%= form_tag("/blueprints", :method => :get) do %>
      <%= text_field_tag :id, nil, :placeholder => "Search blueprints", :autocomplete => :off, :data => { :provide =>"typeahead" }, :class => "form-control" %>
      <%= submit_tag "Search", :class => "btn btn-default" %>
    <% end %>
  </div>
</form>
